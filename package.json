{
  "name": "mcp-cs2ts-server",
  "version": "1.0.0",
  "description": "MCP Server for converting code between different languages",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:stdio": "ts-node src/index.ts --stdio",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "cli": "ts-node src/cli.ts"
  },
  "keywords": [
    "mcp",
    "cursor",
    "code-converter",
    "typescript",
    "csharp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "express": "^4.18.2",
    "openai": "^4.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "nodemon": "^3.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "prettier": "^3.1.0"
  }
} 